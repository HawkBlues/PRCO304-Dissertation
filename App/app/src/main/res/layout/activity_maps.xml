<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
<androidx.constraintlayout.widget.ConstraintLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent">

    <TextView
            android:text="Loading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_TextView_SearchResult1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="44dp" android:layout_marginRight="44dp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_TextView_BranchesMatch"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_TextView_BranchesMatch"
            app:layout_constraintHorizontal_bias="0.01"/>
    <TextView
            android:text="Filtering by:"
            android:layout_width="86dp"
            android:layout_height="19dp" android:id="@+id/MapsActivity_TextView_BranchesMatch"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/ActivityMaps_UserSearch"/>
    <TextView
            android:text="Where to buy:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_TextView_branchesSellTest"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/MapsActivity_TextView_ViewingBranchesText"
            android:textSize="18sp" android:layout_marginTop="12dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_TypePlant_TextView"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_CurrentType_TextViewStatic"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_CurrentType_TextViewStatic"/>
    <TextView
            android:text="Current Plant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/MapsActivity_CurrentPlant_TextView"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_TextView_branchesSellTest"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:textSize="18sp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_TextView_branchesSellTest"/>
    <TextView
            android:text="Type:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/MapsActivity_CurrentType_TextViewStatic"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/MapsActivity_TextView_branchesSellTest"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/MapsActivity_TextView_branchesSellTest"/>
    <TextView
            android:text="All Branches"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_TextView_ViewingBranchesText"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="8dp"
            android:textStyle="bold" android:textSize="24sp"/>
    <Button
            android:text="Show"
            android:layout_width="80dp"
            android:layout_height="40dp" android:id="@+id/MapsActivity_PrevID0_Btn"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/MapsActivity_PrevID0_Desc"/>
    <Button
            android:text="Show"
            android:layout_width="85dp"
            android:layout_height="40dp" android:id="@+id/MapsActivity_PrevID2_Btn"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintTop_toBottomOf="@+id/MapsActivity_PrevID2_Desc"/>
    <Button
            android:text="Show"
            android:layout_width="85dp"
            android:layout_height="40dp" android:id="@+id/MapsActivity_PrevID3_Btn"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintTop_toBottomOf="@+id/MapsActivity_PrevID3_Desc"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_PrevID0_Name"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_PrevID0_Img" android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_PrevID0_Img"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_PrevID0_Desc"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_PrevID0_Name" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_PrevID0_Name"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_PrevID1_Name"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_PrevID1_Img" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_PrevID1_Img"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_PrevID1_Name" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_PrevID1_Name"
            android:id="@+id/MapsActivity_PrevID1_Desc"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_PrevID2_Name"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_PrevID2_Img" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_PrevID2_Img"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_PrevID2_Desc"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_PrevID2_Name" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_PrevID2_Name"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_PrevID3_Name"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_PrevID3_Img" android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_PrevID3_Img"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_PrevID3_Desc"
            app:layout_constraintStart_toEndOf="@+id/MapsActivity_PrevID3_Name" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            app:layout_constraintTop_toTopOf="@+id/MapsActivity_PrevID3_Name"/>
    <ImageView
            android:layout_width="117dp"
            android:layout_height="72dp" app:srcCompat="@android:drawable/btn_star_big_on"
            android:id="@+id/MapsActivity_PrevID0_Img"
            android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="117dp"
            android:layout_height="72dp" app:srcCompat="@android:drawable/btn_star_big_on"
            android:id="@+id/MapsActivity_PrevID1_Img" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/MapsActivity_PrevID0_Img"
            app:layout_constraintStart_toStartOf="@+id/MapsActivity_PrevID0_Img"
    />
    <ImageView
            android:layout_width="117dp"
            android:layout_height="72dp" app:srcCompat="@android:drawable/btn_star_big_on"
            android:id="@+id/MapsActivity_PrevID2_Img" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/MapsActivity_PrevID1_Img"
            app:layout_constraintStart_toStartOf="@+id/MapsActivity_PrevID1_Img"
    />
    <ImageView
            android:layout_width="117dp"
            android:layout_height="72dp" app:srcCompat="@android:drawable/btn_star_big_on"
            android:id="@+id/MapsActivity_PrevID3_Img" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/MapsActivity_PrevID2_Img"
            app:layout_constraintStart_toStartOf="@+id/MapsActivity_PrevID2_Img"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.024"/>
    <EditText
            android:layout_width="291dp"
            android:layout_height="40dp" android:id="@+id/ActivityMaps_UserSearch"
            android:inputType="text"
            android:ems="10"
            android:textColor="#000000"
            android:focusable="true"
            android:hint="Filter by Branch or Type" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:clickable="true" android:editable="true" app:layout_constraintHorizontal_bias="0.057"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/MapsActivity_CurrentType_TextViewStatic"/>
    <Button
            android:text="Show"
            android:layout_width="85dp"
            android:layout_height="40dp" android:id="@+id/MapsActivity_PrevID1_Btn"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintTop_toBottomOf="@+id/MapsActivity_PrevID1_Desc"/>
    <Button
            android:text="Filter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_UserSearch_Btn"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintTop_toTopOf="@+id/ActivityMaps_UserSearch"
            app:layout_constraintStart_toEndOf="@+id/ActivityMaps_UserSearch"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0"/>
    <TextView
            android:text="Your previous identifications"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MapsActivity_TextView1"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout" android:textStyle="bold"/>
    <LinearLayout
            android:layout_height="291dp"
            android:layout_width="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.13999999"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/linearLayout"
            android:layout_marginBottom="216dp"
            app:layout_constraintTop_toBottomOf="@+id/ActivityMaps_UserSearch" android:layout_marginTop="8dp">

        <fragment xmlns:android="http://schemas.android.com/apk/res/android"
                  xmlns:tools="http://schemas.android.com/tools"
                  xmlns:map="http://schemas.android.com/apk/res-auto"
                  android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:id="@+id/map"
                  tools:context=".Activities.MapsActivity"
                  android:name="com.google.android.gms.maps.SupportMapFragment" android:baselineAligned="false"/>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>